<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout
        xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/constraintLayout6">

    <android.support.v7.widget.CardView
            android:layout_width="336dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:id="@+id/cardView3" app:cardElevation="4sp" android:visibility="visible">
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <ImageView
                    android:layout_width="67dp"
                    android:layout_height="67dp" tools:srcCompat="@tools:sample/avatars[14]"
                    android:id="@+id/proposalUserPicture"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/guideline8" android:layout_marginEnd="8dp"
                    android:contentDescription="@string/searchResultActivity_propsalUserAvatarDescription"/>
            <android.support.constraint.Guideline
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/guideline8"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.25"/>
            <TextView
                    android:text="Adrien F."
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/proposalUserName"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/guideline8"
                    android:layout_marginStart="8dp"
                    android:contentDescription="@string/searchResultActivity_propsalUserNameDescription"
                    android:textStyle="bold" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toTopOf="@+id/textView7" android:layout_marginTop="8dp"/>
            <RatingBar
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ratingBar3" style="@style/ratingBarCustom"
                    android:rating="5" app:layout_constraintStart_toEndOf="@+id/proposalUserName"
                    android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="8dp"/>
            <TextView
                    android:text="Actuellement Ã  Romes (ITA)"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:id="@+id/textView7" app:layout_constraintStart_toStartOf="@+id/guideline8"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/ratingBar3"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@android:drawable/btn_star_big_on"
                    android:id="@+id/imageView3"
                    app:layout_constraintStart_toStartOf="@+id/guideline8" android:layout_marginStart="8dp"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                    android:contentDescription="@string/searchResultActivity_availableProductIcon"
                    app:layout_constraintTop_toBottomOf="@+id/textView7" android:layout_marginTop="8dp"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@android:drawable/btn_star_big_on"
                    android:id="@+id/imageView4"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageView3"
                    android:contentDescription="@string/searchResultActivity_availableProductIcon"
                    app:layout_constraintTop_toBottomOf="@+id/textView7" android:layout_marginTop="8dp"/>
            <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp" app:srcCompat="@android:drawable/btn_star_big_on"
                    android:id="@+id/imageView6"
                    android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageView4"
                    android:contentDescription="@string/searchResultActivity_availableProductIcon"
                    app:layout_constraintTop_toBottomOf="@+id/textView7" android:layout_marginTop="8dp"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_menu_mylocation"
                    android:id="@+id/imageView7" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:contentDescription="@string/searchResultActivity_proposalLocationIcon"
                    app:layout_constraintStart_toEndOf="@+id/textView7" android:layout_marginStart="8dp"/>
            <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_menu_mylocation"
                    android:id="@+id/imageView8"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/textView7"
                    android:layout_marginStart="8dp"
                    android:contentDescription="@string/searchResultActivity_proposalLocationIcon"
                    app:layout_constraintTop_toBottomOf="@+id/imageView7"
                    android:layout_marginBottom="8dp"/>
        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>
</android.support.constraint.ConstraintLayout>